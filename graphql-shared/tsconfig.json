{
  "compilerOptions": {
    "composite": true,
    "target": "es2016",
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "sourceMap": false,
    "declaration": true,
    "declarationMap": false,
    "removeComments": true,
    "allowJs": true,
    "resolveJsonModule": false,
    "strict": false,
    "noImplicitAny": false,
    "noImplicitReturns": false,
    "noFallthroughCasesInSwitch": true,
    "noUnusedParameters": false,
    "noUnusedLocals": false,
    "forceConsistentCasingInFileNames": true,
    "rootDir": "./src",
    "outDir": "./lib",
    "lib": [],
    "types": ["node"],
    "baseUrl": "."
  },
  "include": ["src"],
  "exclude": [
    "node_modules"
  ]
}